<div class="center">
  
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-md-3">
        <div class="thumbnail">
          
          <div class="caption">
            <h3><%= listing.name %></h3>
            <p><%= number_to_currency(listing.price) %></p>
            <p><%= "Sold by #{listing.user.name}" %></p>
            <% if (user_signed_in?) && (current_user == listing.user) %>
            <% end %>
          </div>
        </div>
      </div>
      <% end %>
  </div>
</div>

<br>

